{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "parser": "@babel/eslint-parser",
    "rules": {
        "quotes": [
            "error",
            "single"
        ],
        "semi": "error"
    }
}
